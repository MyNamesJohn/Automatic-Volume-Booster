{
  "manifest_version": 3,
  "name": "Video Volume Booster",
  "version": "1.0",
  "description": "Automatically increases the volume of videos.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}